  "version": "0.0.1",
  "require": {
    "illuminate/support": "^8.26",
    "laravel/jetstream": "^2.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ozzie\\Nextstream\\NextstreamServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Ozzie\\Nextstream\\": "src/"
    }
  }
}
